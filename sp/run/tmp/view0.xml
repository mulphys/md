			-->
			<specie id="O2">
				<density title="kg/m^3: 0.2805846">0.2805846</density>
			</specie>
			<specie id="N2">
				<density title="kg/m^3">0.9118998</density>
			</specie>
<!--
			<specie id="H2O">
				<density title="kg/m^3">0.0075157</density>
			</specie>
-->
		</boundary>
		<boundary id="front"> <type>elastic</type></boundary>
		<boundary id="rear"> <type>elastic</type></boundary>
	</domain>
	<!-- GUI -->
	<gui>
		<translation>
			<step>0.01</step>
		</translation>
		<vector>
			<length>2.0</length>
			<thickness>1.0</thickness>
			<color>
				<red>0.5</red>
				<green>1.0</green>
				<blue>1.0</blue>
			</color>
		</vector>
		<frame>
			<line>
				<thickness>1.0</thickness>
				<color>
					<red>1.0</red>
					<green>0.0</green>
					<blue>0.0</blue>
				</color>
			</line>
		</frame>
		<mesh>
			<!-- Possible Color Schemes:
				0: by boundary
				1: by time
			-->
			<node>
				<type title="types: spere point">sphere</type>
				<!--<type>point</type>-->
				<colorscheme>
					<variable text="Values: mass type">type</variable>
					<minvalue>0.0</minvalue>
					<maxvalue>4.0</maxvalue>
				</colorscheme>
				<size>1.0</size>
				<value>
					<min>-1.0</min>
					<max>1.0</max>
				</value>
				<color>
					<!-- Basic colors: -->
					<red>0.0</red>
					<green>1.0</green>
					<blue>0.0</blue>
				</color>
			</node>
			<line>
				<thickness>1.0</thickness>
				<color>
					<red>1.0</red>
					<green>0.5</green>
					<blue>0.0</blue>
				</color>
				<boundary_color>
					<red>0.0</red>
					<green>1.0</green>
					<blue>0.5</blue>
				</boundary_color>
			</line>
		</mesh>
	</gui>	
	<xterm>1</xterm>
</remody>
